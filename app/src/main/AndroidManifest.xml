<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ilo.ilo">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LogInActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".ViewProfileActivity" />
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".ViewTasks" />
        <activity android:name=".TaskActivity" />
        <activity android:name=".BrowseActivity" />
        <activity android:name=".ViewModulesActivity" />
        <activity android:name=".UserSearchActivity" />
        <activity android:name=".LeaderBoardActivity"></activity>
    </application>

</manifest>